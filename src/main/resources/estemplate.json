{
    "template": "loghub-*",
    "settings": {
      "index.analysis.analyzer.default.stopwords": "_none_",
      "index.refresh_interval": "5s",
      "index.analysis.analyzer.default.type": "standard"
    },
    "mappings": {
      "_default_": {
        "dynamic_templates": [
          {
            "string_fields": {
              "mapping": {
                "type": "multi_field",
                "fields": {
                  "{name}": {
                    "index": "analyzed",
                    "omit_norms": true,
                    "type": "string"
                  },
                  "raw": {
                    "ignore_above": 256,
                    "index": "not_analyzed",
                    "type": "string"
                  }
                }
              },
              "match_mapping_type": "string",
              "match": "*"
            }
          }
        ],
        "_all": {
          "enabled": true
        },
        "properties": {
        }
      }
    },
    "aliases": {}
}